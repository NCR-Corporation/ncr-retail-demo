{
  "name": "sample-app-retail-demo",
  "version": "2.0.0",
  "private": true,
  "description": "A sample retail demo application built on top of NCR's APIs.",
  "homepage": "https://github.com/NCR-Corporation/ncr-retail-demo",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 8080",
    "find:unused": "next-unused",
    "prettify": "npx prettier --write .",
    "postinstall": "patch-package",
    "cy:open": "cypress open"
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@public/*": [
        "public/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@context/*": [
        "context/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.3",
    "cookie": "^0.4.1",
    "cookie-cutter": "^0.2.0",
    "formik": "^2.2.6",
    "framer-motion": "^3.7.0",
    "google-map-react": "^2.1.9",
    "haversine": "^1.1.1",
    "js-base64": "^3.6.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "ncr-hmac": "file:ncr-hmac",
    "next": "10.0.3",
    "next-auth": "3.1.0",
    "nprogress": "^0.2.0",
    "react": "17.0.1",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-filter": "^1.3.3",
    "react-datepicker": "^3.3.0",
    "react-dom": "17.0.1",
    "react-draggable": "^4.4.3",
    "react-geolocated": "^3.1.0",
    "react-json-view": "git+https://github.com/thomasjm/react-json-view.git#e9baddc761536929f436e93baa5cebc5be519417",
    "react-loading-skeleton": "^2.1.1",
    "react-payment-inputs": "^1.1.7",
    "react-resizable": "^1.11.0",
    "reactstrap": "^8.8.1",
    "request": "^2.88.2",
    "sass": "^1.32.7",
    "swr": "^0.3.11",
    "uniqid": "^5.2.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "cypress": "^6.5.0",
    "next-unused": "0.0.3",
    "patch-package": "^6.2.2",
    "prettier": "^2.2.1"
  }
}
